{
  "name": "@tiddlybase/functions",
  "description": "firebase functions",
  "version": "0.0.0",
  "private": true,
  "author": "Peter Neumark",
  "license": "Apache-2.0",
  "scripts": {
    "build": "./build.sh",
    "local": "./run.sh",
    "build:dev": "MODE=development ./build.sh",
    "lint": "eslint --ext .js,.ts .",
