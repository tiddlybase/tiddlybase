    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.1",
    "nodemailer": "^6.7.2",
    "source-map-support": "^0.5.21",
    "tslib": "^2.3.1"
