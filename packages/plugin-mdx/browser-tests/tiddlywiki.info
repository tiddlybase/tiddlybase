{
    "description": "Test runner",
    "includeWikis": [
		{"path": "../../browser-tests", "read-only": true}
	],
    "config": {
        "parse-markdown-as-mdx": true
    },
    "plugins": [
        "tiddlybase/tiddlybase-utils",
        "tiddlybase/browser-test-utils",
        "tiddlybase/react",
        "tiddlybase/embed-url",
        "tiddlybase/mdx"
    ],
    "themes": [
    ],
    "build": {
        "wiki": [
            "--rendertiddler",
            "$:/core/save/all",
            "wiki.html",
            "text/plain"
        ]
    }
}
