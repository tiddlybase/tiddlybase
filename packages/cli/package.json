{
    "name": "@tiddlybase/cli",
    "description": "In-browser trusted code running outside the sandbox",
    "version": "0.0.23",
    "author": "Peter Neumark",
    "license": "Apache-2.0",
    "bin": {
        "tiddlybase": "dist/tiddlybase"
    },
    "files": [
        "dist/**",
        "templates/**"
    ],
    "main": "dist/tiddlybase",
    "scripts": {
        "clean": "rm -rf dist/*",
        "build": "webpack && chmod +x dist/tiddlybase",
        "build:dev": "MODE=development webpack",
        "cli": "ts-node src/cli.ts",
        "publish": "yarn npm publish --access public"
    },
    "devDependencies": {
        "@tiddlybase/shared": "workspace:*",
        "@tiddlybase/webpack-config": "workspace:*",
        "@types/lodash.merge": "^4.6.7",
        "@types/mustache": "^4.2.1",
        "@types/node": "^17.0.23",
        "@types/webpack-env": "^1.18.1",
        "@types/yargs": "^17.0.8",
        "ajv": "^8.12.0",
        "firebase": "^10.12.3",
        "ts-node": "^10.5.0",
        "typescript": "^5.4.3",
        "webpack": "^5.91.0"
    },
    "dependencies": {
        "ajv-formats": "^2.1.1",
        "firebase-admin": "^11.10.1",
        "json5": "^2.2.3",
        "lodash.merge": "^4.6.2",
        "mustache": "^4.2.0",
        "tiddlywiki": "^5.3.1",
        "tslib": "^2.6.1",
        "yargs": "^17.3.1"
    }
}
